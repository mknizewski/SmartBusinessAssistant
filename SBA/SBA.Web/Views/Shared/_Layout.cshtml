@using SBA.Web.Infrastructure.Alert

<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title — WebCoding S.A</title>
    @Styles.Render("~/Content/mainCss")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="loader" id="loadingSpinner" style="display: none;">
    </div>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Główna", "Index", "Home", new { @class = "navbar-brand" })
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li>
                        @Html.ActionLink("O nas", "About", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("Aktualności", "Index", "News")
                    </li>
                    <li>
                        @Html.ActionLink("Kontakt", "Index", "Contact")
                    </li>
                </ul>

				<div class="col-xs-12 col-lg-4 col-sm-5 col-md-4 pull-right">
					<form class="navbar-form" role="search">
					<div class="input-group" id="searchInputGroup">
						<input type="text" class="form-control" placeholder="Search" name="searchInput" id="searchInput">
						<div class="input-group-btn">
							<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
						</div>
					</div>
					</form>
				</div>

            </div>
        </div>
    </nav>
    <header class="business-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="tagline">WebCoding S.A.</h1>
					<p class="taglineDescribe">Bo programowanie to nasza pasja...</p>
				</div>
            </div>
        </div>
    </header>
    <div class="container">
        <br />
        <div id="tempMessage">
            @Html.RenderAlert()
        </div>
        @RenderBody()
    </div>
	

	<footer class="page-footer font-small stylish-color-dark pt-4 mt-4 bg-dark">

			<!--Footer Links-->
			<div class="container mt-5 mb-4">
				<div class="row mt-3">

					<!--First column-->
					<div class="col-md-3 col-lg-4  mb-4">
						<h6 class="text-uppercase font-weight-bold ">
							<strong>WebCoding S.A.</strong>
						</h6>
						<hr class="deep-purple accent-2 mb-4 mt-0 d-inline-block mx-auto h6hr">
						<p>  Nasza wspaniała firma zajmuje się tym i tym. Jesteśmy obecni już na rynku od 2010 roku. Nasze projekty były chwalone, a realizacja z klientem zawsze przebiegała pomyślnie. <br>Zachęcamy do skorzystania z naszych usług!</p>
					</div>
					<!--/.First column-->

					<!--Second column-->
					<div class="col-md-3 col-lg-2  mx-auto mb-4">
						<h6 class="text-uppercase font-weight-bold">
							<strong>Produkty</strong>
						</h6>
						<hr class="deep-purple accent-2 mb-4 mt-0 d-inline-block mx-auto h6hr">
						<p>
							<a href="#!">MDBootstrap</a>
						</p>
						<p>
							<a href="#!">MDWordPress</a>
						</p>
						<p>
							<a href="#!">BrandFlow</a>
